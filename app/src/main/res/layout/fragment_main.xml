<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.PlaceholderFragment">

    <Button
        android:id="@+id/button6"
        android:layout_width="142dp"
        android:layout_height="68dp"
        android:background="#8BC34A"
        android:text="+"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="459dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="553dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="142dp"
        android:layout_height="68dp"
        android:background="#8BC34A"
        android:text="+"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="459dp" />

    <TextView
        android:id="@+id/section_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/activity_horizontal_margin"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginEnd="@dimen/activity_horizontal_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="233dp"
        android:layout_height="38dp"
        android:text="Starting Position"
        android:textAlignment="textEnd"
        android:textColor="#045206"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="53dp" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="170dp"
        android:layout_height="193dp"
        android:background="#3F51B5"
        android:fontFamily="sans-serif-smallcaps"
        android:text="ToggleButton"
        android:textOff="LVL 1"
        android:textOn="LVL 1"
        android:textSize="36sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="125dp" />

    <ToggleButton
        android:id="@+id/toggleButton2"
        android:layout_width="170dp"
        android:layout_height="190dp"
        android:background="#F44336"
        android:fontFamily="sans-serif-smallcaps"
        android:text="ToggleButton"
        android:textOff="LVL 2"
        android:textOn="LVL 2"
        android:textSize="36sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="127dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="277dp"
        android:layout_height="57dp"
        android:text="Pass Auto Line"
        android:textColor="#066F7E"
        android:textSize="36sp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="333dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="86dp"
        android:layout_height="38dp"
        android:text="Hatch"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="401dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="90dp"
        android:layout_height="39dp"
        android:text="Cargo"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="401dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="142dp"
        android:layout_height="68dp"
        android:background="#8BC34A"
        android:text="+"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="647dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="138dp"
        android:layout_height="56dp"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="553dp" />

</androidx.constraintlayout.widget.ConstraintLayout>