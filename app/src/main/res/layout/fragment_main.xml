        tools:layout_editor_absoluteY="479dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="59dp"
        android:layout_height="59dp"
        android:text="1"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="606dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="59dp"
        android:layout_height="59dp"
        android:fontFamily="monospace"
        android:text="Cargo"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="606dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="88dp"
        android:layout_height="61dp"
        android:background="@drawable/ic_launcher_background"
        android:text="+"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="465dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="88dp"
        android:layout_height="61dp"
        android:background="@drawable/ic_launcher_background"
        android:text="+"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="298dp"
        tools:layout_editor_absoluteY="590dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="88dp"
        android:layout_height="61dp"
        android:background="@drawable/ic_launcher_background"
        android:text="-"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="462dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="88dp"
        android:layout_height="61dp"
        android:background="@drawable/ic_launcher_background"
        android:text="-"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="590dp" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="161dp"
        android:layout_height="147dp"
        android:background="@drawable/mybuttonred"
        android:fontFamily="monospace"
        android:text="ToggleButton"
        android:textColor="#FFFFFF"
        android:textOff="LVL 1"
        android:textOn="LVL 1"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="145dp" />

    <ToggleButton
        android:id="@+id/toggleButton2"
        android:layout_width="160dp"
        android:layout_height="145dp"
        android:background="@drawable/mybuttonblue"
        android:fontFamily="monospace"
        android:text="ToggleButton"
        android:textColor="#000000"
        android:textOff="LVL 2"
        android:textOn="LVL 2"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="145dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="_____________________________________________________"
        android:textColor="#FF5722"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="313dp" />

</androidx.constraintlayout.widget.ConstraintLayout>