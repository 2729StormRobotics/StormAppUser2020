<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/endgame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context="org.stormroboticsnj.stormuserradar2020.Endgame"
    tools:visibility="visible">

    <Button
        android:id="@+id/btnEMoreLVL1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="168dp"
        android:background="@drawable/my_button_orange"
        android:fontFamily="sans-serif"
        android:text="@string/plus"
        android:textSize="70sp"
        android:typeface="serif"
        app:layout_constraintStart_toEndOf="@+id/btnELessLVL1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnELessLVL1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="168dp"
        android:background="@drawable/my_button_red"
        android:fontFamily="sans-serif"
        android:text="@string/minus"
        android:textSize="100sp"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnEMoreLVL3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="268dp"
        android:background="@drawable/my_button_orange"
        android:fontFamily="sans-serif"
        android:text="@string/plus"
        android:textColor="#000000"
        android:textSize="70sp"
        android:typeface="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.99" />

    <TextView
        android:id="@+id/txtEScoredLVL3"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_marginStart="404dp"
        android:layout_marginBottom="348dp"
        android:fontFamily="@font/lato"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtEScoredLVL2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="404dp"
        android:layout_marginBottom="680dp"
        android:fontFamily="@font/lato"
        android:text="0"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtEScoredLVL1"
        android:layout_width="wrap_content"
        android:layout_height="76dp"
        android:layout_marginStart="404dp"
        android:layout_marginBottom="956dp"
        android:fontFamily="@font/lato"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnEMoreLVL2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginTop="484dp"
        android:layout_marginEnd="120dp"
        android:background="@drawable/my_button_orange"
        android:fontFamily="sans-serif"
        android:text="@string/plus"
        android:textSize="70sp"
        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnELessLVL2"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="484dp"
        android:background="@drawable/my_button_red"
        android:fontFamily="sans-serif"
        android:text="@string/minus"
        android:textSize="100sp"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnELessLVL3"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="804dp"
        android:background="@drawable/my_button_red"
        android:fontFamily="sans-serif"
        android:text="@string/minus"
        android:textSize="100sp"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnSubmit"
        android:layout_width="195dp"
        android:layout_height="94dp"
        android:layout_marginEnd="300dp"
        android:layout_marginBottom="48dp"
        android:background="#12B815"
        android:fontFamily="@font/lato"
        android:text="Submit"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="244dp"
        android:fontFamily="@font/lato"
        android:text="Bottom Port"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="288dp"
        android:layout_height="88dp"
        android:layout_marginTop="696dp"
        android:layout_marginEnd="220dp"
        android:fontFamily="@font/lato"
        android:text="Inner Port"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="400dp"
        android:layout_marginEnd="252dp"
        android:fontFamily="@font/lato"
        android:text="Outer Port"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="1030dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="wrap_content"
            android:layout_height="92dp"

            android:text="Hang"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="256dp"
            android:layout_height="92dp"

            android:text="Level Hang"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintBottom_toTopOf="@+id/btnSubmit"
            app:layout_constraintEnd_toEndOf="parent" />

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="180dp"
            android:layout_height="match_parent"

            android:fontFamily="@font/lato"
            android:text="Park"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/radioButton3"
            app:layout_constraintTop_toBottomOf="@+id/btnEMoreLVL3"
            app:layout_constraintVertical_bias="0.556" />
    </RadioGroup>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.52" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.15" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.85" />

</androidx.constraintlayout.widget.ConstraintLayout>